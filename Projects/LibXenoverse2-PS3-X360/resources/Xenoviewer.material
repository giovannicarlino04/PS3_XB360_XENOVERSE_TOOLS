////////////////////////////////////////////////////////////////////////////////
material VertexColours
{
	technique
	{
		pass
		{
			ambient vertexcolour
			diffuse vertexcolour
			specular vertexcolour
			lighting off
			cull_hardware none
			cull_software none
			//separate_scene_blend one one_minus_src_alpha one one_minus_src_alpha
			depth_write off
			depth_check off
      //transparent_sorting force
		}
	}
}


