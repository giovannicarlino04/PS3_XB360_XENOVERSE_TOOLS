////////////////////////////////////////////////////////////////////////////////
material SymmetrySolved
{
	technique
	{
		pass
		{
			lighting off
      depth_check off
			
			/*
      fragment_program_ref symmetrySolved_ps
      {
      }
      */

			texture_unit
      {
          texture empty.png
          tex_address_mode clamp
          filtering linear linear none
          colour_op replace
          tex_coord_set 0
      }
		}
	}
}


